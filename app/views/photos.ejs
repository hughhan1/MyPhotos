  <script type="text/javascript" src="/js/grayscale.js"></script>

  <script type="text/javascript" src="/js/angular/angular.js"></script>
  <script type="text/javascript" src="/js/angular-route/angular-route.min.js"></script>
  <script type="text/javascript" src="/js/angular-resource/angular-resource.min.js"></script>

  <script type="text/javascript" src="/photos/photos.client.module.js"></script>
  <script type="text/javascript" src="/photos/controllers/photos.client.controller.js"></script>
  <script type="text/javascript" src="/photos/services/photos.client.service.js"></script>
  <script type="text/javascript" src="/photos/config/photos.client.routes.js"></script>

  <script type="text/javascript" src="/replies/replies.client.module.js"></script>
  <script type="text/javascript" src="/replies/controllers/replies.client.controller.js"></script>
  <script type="text/javascript" src="/replies/services/replies.client.service.js"></script>
